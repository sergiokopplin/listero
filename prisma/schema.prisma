generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Task {
  id        Int     @id @default(autoincrement())
  title     String
  completed Boolean
  project   Project @relation(fields: [projectId], references: [id])
  projectId Int
}

model Project {
  id    Int    @id @default(autoincrement())
  title String
  Task  Task[]
}
